<div class="container" id="casting">
    <div class="row">
        <div class="col-3">
            <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/f/f1/Bill_Murray-8882.jpg/1200px-Bill_Murray-8882.jpg" alt="profile pic" width="200" height=""><br></div>
        <div class="col-9" id="casting">
            <h1 id="casting"><%= @casting_agent.name %></h1>
            Agency: <%= @casting_agent.agency %><br>
        </div>
    </div>
    <div class="row">
        <div class="col-3">
            Events:<br>
            <% @casting_agent.events.each do |event| %>
                <%= link_to event.title, event_path(event) %><br>
            <% end %>
        </div>
        <div class="col-9" id="casting">
            About: <br>
            <br>
        </div>
    </div>
</div>


