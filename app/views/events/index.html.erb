<h1>All Events</h1>

<%= form_tag(events_path, method: "get") do %>
  <%= label_tag(:q, "Search by Casting Agent:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>

<% @events.each do |event| %>
    <%= link_to event.title, event_path(event) %><br>
    <%= event.casting_agent.name %><br>
<% end %>